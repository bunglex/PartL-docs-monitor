<!doctype html><meta charset=""utf-8""><title>Building Energy Docs Registry</title>
<h1>Current Energy/Part L Documents</h1>
<table border=""1"" cellpadding=""6"" cellspacing=""0"">
<tr><th>Jurisdiction</th><th>Document</th><th>Rule Version</th><th>Last Seen</th><th>PDF</th><th>History</th></tr>
{% for it in items %}
<tr>
  <td>{{ it.jurisdiction|replace(""_"","" "")|title }}</td>
  <td>{{ it.name }}</td>
  <td>{{ it.rule_version }}</td>
  <td>{{ it.last_seen }}</td>
  <td><a href=""{{ it.source_url }}"">Open PDF</a></td>
  <td><a href=""./history/{{ it.slug }}.html"">View history</a></td>
</tr>
{% endfor %}
</table>
<p>Machine-readable: <a href=""./current.json"">current.json</a></p>
